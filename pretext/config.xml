<?xml version="1.0" encoding="utf-8"?>
<pretext
    lang="en"
    dir="../../dataset/economics/result"
    log="pretext.log"
    silence="off">

    <maid>
        <number/>
        <html/>
        <simbols/>
        <stoplist dir="stoplist">
            <stopfile>ingl.xml</stopfile>
        </stoplist>
        <stemming dir="steminfo"/>
    </maid>

    <ngram dir="ngraminfo">
        <gram n="1"/>
    </ngram>

    <report
        ngramdir="ngraminfo"
        discover="discover"
        taxonomy="taxonomia.txt"
        transpose="disabled">

        <gram n="1"
            max="500"
            min="10"
            measure="tfidf"
            smooth="disabled"/>
    </report>
</pretext>
